{
  "name": "jkweb/symfony-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "A fully configured symfony framework for JKwebGmbH projects",
  "require": {
    "php": "^7.3",
    "ext-bcmath": "*",
    "ext-curl": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-simplexml": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "jms/serializer-bundle": "^3.4",
    "sensio/framework-extra-bundle": "^5.1",
    "sensiolabs/security-checker": "^5.0",
    "stof/doctrine-extensions-bundle": "^1.3",
    "symfony/asset": "^4.3",
    "symfony/console": "^4.3",
    "symfony/dotenv": "^4.3",
    "symfony/expression-language": "^4.3",
    "symfony/flex": "^1.3",
    "symfony/form": "^4.3",
    "symfony/framework-bundle": "^4.3",
    "symfony/monolog-bundle": "^3.3",
    "symfony/orm-pack": "^1.0",
    "symfony/property-info": "^4.3",
    "symfony/security-bundle": "^4.3",
    "symfony/serializer": "^4.3",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^4.3",
    "symfony/templating": "^4.3",
    "symfony/twig-bundle": "^4.3",
    "symfony/validator": "^4.3",
    "symfony/web-link": "^4.3",
    "symfony/webpack-encore-bundle": "^1.7",
    "symfony/yaml": "4.3",
    "fr3d/ldap-bundle": "^4.0",
    "friendsofsymfony/user-bundle": "^2.1",
    "friendsofsymfony/jsrouting-bundle": "^2.4",
    "bcc/cron-manager-bundle": "^4.2",
    "gos/web-socket-bundle": "^1.10",
    "suncat/mobile-detect-bundle": "^1.1",
    "fresh/firebase-cloud-messaging-bundle": "^0.4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "symfony/apache-pack": "^1.0",
    "symfony/debug-bundle": "^4.3",
    "symfony/test-pack": "^1.0",
    "symfony/web-profiler-bundle": "^4.3",
    "symfony/web-server-bundle": "^4.3"
  },
  "config": {
    "platform": {
      "php": "7.3.0"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "security-checker security:check": "script"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "require": "4.3.*",
      "allow-contrib": false
    }
  }
}
